{
	"manifest_version": 2, 
 	 "name": "RateMyGold",
 	 "description": "Choose your UCSB courses more efficiently with ratings from RateMyProfessors.com.", 
  	"version": "1.0",

 	 "permissions": ["http://*/*","https://*/*"],

  	"background": {
    	"scripts": ["professorPage.js"],
    	"persistent": false
 	 },

 	 "content_scripts": [
		  {
		  "matches": ["https://my.sa.ucsb.edu/gold/ResultsFindCourses.aspx"],
		  "css": ["sheet.css"],
		  "js": ["main.js"]
	 	 }
 	 ]
}

